{
  "name": "@awint/bitbucket-codebuild-cdk-construct",
  "version": "1.0.0",
  "description": "AWS CDK Construct for creating CodeBuild for BitBucket projects",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -b .",
    "clean": "tsc -b --clean",
    "watch": "tsc -b -w",
    "jsii": "jsii",
    "package": "jsii-pacmak"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/awintbit/aws-construct.git"
  },
  "keywords": [
    "aws",
    "cdk",
    "construct",
    "codebuild",
    "ci",
    "bitbucket"
  ],
  "author": "Andrew Wint",
  "license": "Apache-2.0",
  "homepage": "https://bitbucket.org/awintbit/aws-construct#readme",
  "stability": "experimental",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "full"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.86.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "^1.86.0",
    "jest": "^26.4.2",
    "jsii": "^1.16.0",
    "jsii-pacmak": "^1.16.0",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-codebuild": "^1.86.0",
    "@aws-cdk/aws-s3": "^1.86.0",
    "@aws-cdk/core": "^1.86.0"
  }
}
